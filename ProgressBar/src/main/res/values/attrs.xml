<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TierProgress">

        <attr name="tier_progress" format="integer" />
        <attr name="tier_progress_stroke_width" format="dimension" />
        <attr name="tier_progress_bar_color" format="color"/>
        <attr name="tier_progress_bar_main_color" format="color"/>
        <attr name="tier_progress_bar_indicator_color" format="color"/>
        <attr name="tier_progress_bar_diamond_step_color" format="color"/>
        <attr name="tier_progress_bar_label_color" format="color"/>
        <attr name="tier_progress_bar_sub_label_color" format="color"/>
        <attr name="tier_progress_bar_label_enabled" format="boolean"/>
        <attr name="tier_progress_bar_sub_label_enabled" format="boolean"/>
        <attr name="tier_progress_bar_label_mode" format="enum">
            <enum name="none" value="0"/>
            <enum name="all" value="1"/>
            <enum name="sides" value="2"/>
            <enum name="start" value="3"/>
            <enum name="end" value="4"/>
            <enum name="mid" value="5"/>
        </attr>
        <attr name="tier_progress_bar_label_font_size" format="integer"/>
        <attr name="tier_progress_bar_sub_label_font_size" format="integer"/>

    </declare-styleable>

</resources>